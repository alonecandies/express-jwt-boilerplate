    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "rootpath": "^0.1.2",
    "sequelize": "^6.12.5",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.4.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0",
