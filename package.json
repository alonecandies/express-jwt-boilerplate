{
  "name": "expressjs-mysql-authentication-crud-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate powered by ExpressJs, MySQL for Authentication and CRUD",
  "main": "src/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "lint": "npx eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src test --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alonecandies/expressjs-mysql-authentication-crud-boilerplate.git"
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "mysql",
    "orm",
    "authentication",
    "crud"
  ],
  "author": "alonecandies",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alonecandies/expressjs-mysql-authentication-crud-boilerplate/issues"
  },
  "homepage": "https://github.com/alonecandies/expressjs-mysql-authentication-crud-boilerplate#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
